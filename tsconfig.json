{
  "extends": "@tsconfig/node22/tsconfig.json",
  "compileOnSave": true,
  "compilerOptions": {
    "isolatedModules": true,
    "baseUrl": "integration_tests/playwright/test",
    "paths": {
      "pages/launchpad/launchlinks": ["pages/Launchpad_Portal/launchpadExternalLinksLocators.ts"]
    },
    "outDir": "./dist",
    "sourceMap": true,
    "skipLibCheck": true,
    "noEmit": false,
    "allowJs": false,
    "strict": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "typeRoots": ["./server/@types", "./node_modules/@types"]
  },
  "exclude": ["node_modules", "assets/**/*.js", "dist", "cypress.config.ts", "eslint.config.mjs"],
  "include": ["**/*.js", "**/*.ts", "**/*.json", "integration_tests/playwright/test/pages/Launchpad_Portal/*.ts"]
}
