{
  "compilerOptions": {
    "target": "es5",
    "noEmit": true,
    "lib": ["es5", "dom", "es2015.promise"],
    "types": ["cypress", "express", "express-session"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "playwright/test",
    "paths": {
      "pages/launchpad/launchlinks": ["pages/Launchpad_Portal/launchpadExternalLinksLocators"],
      "pages/launchpad/portal": ["pages/Launchpad_Portal/launchpadPortalLocators"]
    }
  },
  "include": ["**/*.ts", "support/playwright.global-setup.js", "support/playwright.global-teardown.js"]
}
