{
  "compilerOptions": {
    "target": "es2022",
    "noEmit": true,
    "lib": ["es2022", "dom"],
    "types": ["@playwright/test", "express", "express-session"],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "pages/launchpad/launchlinks": ["playwright/test/pages/Launchpad_Portal/launchpadExternalLinksLocators"],
      "pages/launchpad/portal": ["playwright/test/pages/Launchpad_Portal/launchpadPortalLocators"]
    }
  },
  "include": [
    "playwright/**/*.ts",
    "support/playwright.global-setup.js", 
    "support/playwright.global-teardown.js"
  ],
  "exclude": [
    "e2e/**/*",
    "mockApis/**/*",
    "pages/**/*",
    "support/commands.ts",
    "support/index.ts"
  ]
}
